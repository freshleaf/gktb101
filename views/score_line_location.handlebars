{{#section 'head'}}
    <title>高考填报101 历年地区批次分数线</title>
    <meta name="description" content="查看历年高考地区批次分数线">
    <meta name="keywords" content="查看历年高考地区批次分数线">
{{/section}}

{{> header}}
<main role="main" class="container">
    <form class="form-inline mt-4">
        <select name="location" class="custom-select mr-sm-2">
            <option value="">不限</option>
            {{#each location}}
                <option value="{{code}}">{{name}}</option>
            {{/each}}
        </select>
        <!--<input type="submit"/>-->
        <button type="submit" class="btn btn-primary">查询省控线</button>
    </form>
    <br>
    <table class="table table-bordered table-hover">
        <thead class="thead-light">
        <tr>
            <th></th>
            <th>年份</th>
            <th>地区</th>
            <th>文理分科</th>
            <th>批次</th>
            <th>分数线</th>
        </tr>
        </thead>
        <tbody>
        {{#each scoreline}}
            <tr>
                <td>{{no}}</td>
                <td>{{year}}</td>
                <td>{{location}}</td>
                <td>{{student_type}}</td>
                <td>{{batch}}</td>
                <td>{{socre}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</main>
{{> footer}}